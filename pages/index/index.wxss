/**index.wxss**/
.container{
  width: 100%;
}
.container swiper-item image{
  width: 100%;
  height: 100%;
}
.list{
  width: 700rpx;margin: 40rpx auto 0;
}
.list .h1 text{
  font-size: 40rpx;color:#000000;
}
.list .ul{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.list .item{
  width: 26%;
  margin:10rpx 0;
  line-height: 80rpx;
  font-size: 32rpx;
  background: #ccc;
  text-align: center;
  color:#333;
  box-shadow: 0 2px 4px #333;
}
.list .item:last-child{
  width: 100%; 
  
}